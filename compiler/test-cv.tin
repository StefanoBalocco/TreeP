(defnet test-cv (func)
        (deflocal i)

        (lmatch remove func "cv-")
        (for i in (test-cv-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_cv_" (dash->underscore func) "(") "))")
        (flag-true "cv") )

(defun test-cv-table ()
        [ [ "pix-gray"                  1 1 ]
          [ "pix-smooth"                6 6 ]
          [ "pix-warp-affine"           3 4 ]
        ] )

