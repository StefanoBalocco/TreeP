(defnet test-aud (func)
        (deflocal i)

        (lmatch remove func "aud-")
        (for i in (test-aud-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_aud_" (dash->underscore func) "(") "))")
        (flag-true "aud") )

(defun test-aud-table ()
        [ [ "parse-aac-header"  2 2 ]
          [ "parse"             2 3 ]
          [ "parse-step"        1 1 ]
          [ "fpout-begin"       2 3 ]
          [ "fpout-end"         1 1 ]
        ] )

