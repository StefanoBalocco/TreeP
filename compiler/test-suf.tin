(defnet test-suf (func)
        (deflocal i)

        (lmatch remove func "suf-")
        (for i in (test-suf-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_suf_" (dash->underscore func) "(") "))")
        (flag-true "suf") )

(defun test-suf-table ()
        [ [ "xxx"       2 2 ]
        ] )

