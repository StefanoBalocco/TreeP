(defnet test-gcrypt (func)
        (deflocal i)

        (lmatch remove func "gcry-")
        (for i in (test-gcrypt-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_gcry_" (dash->underscore func) "(") "))")
        (flag-true "gcrypt") )

(defun test-gcrypt-table ()
        [ [ "stego-insert"      3 3 ]
          [ "stego-destroy"     2 2 ]
        ] )

