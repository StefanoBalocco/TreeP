(defnet test-avi (func)
        (deflocal i)

        (lmatch remove func "avi-")
        (for i in (test-avi-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (if (= func "video-read")
        then    (set func "video-read-test") )
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_avi_" (dash->underscore func) "(") "))")
        (flag-true "avi") )

(defun test-avi-table ()
        [ [ "set-audio-track"           2 2 ]
          [ "video-read"                3 3 ]
        ] )

