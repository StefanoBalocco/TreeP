(defnet test-mysql (func)
        (deflocal i)

        (lmatch remove func "mysql-")
        (for i in (test-mysql-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_mysql_" (dash->underscore func) "(") "))")
        (flag-true "mysql") )

(defun test-mysql-table ()
        [ ;[ "exec-data" 2 2 ]
        ] )

