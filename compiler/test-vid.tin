(defnet test-vid (func)
        (deflocal i)

        (lmatch remove func "vid-")
        (for i in (test-vid-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_vid_" (dash->underscore func) "(") "))")
        (flag-true "vid") )

(defun test-vid-table ()
        [ [ "parse"                     2 3 ]
          [ "parse-matroska"            2 2 ]
          [ "mp4-load-sample-size"      4 4 ]
          [ "mp4-load-sample-to-chunk"  2 2 ]
          [ "mp4-load-chunk-offset"     3 3 ]
        ] )

