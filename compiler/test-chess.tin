(defnet test-chess (func)
        (deflocal i)

        (lmatch remove func "chess-")
        (for i in (test-chess-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_chess_" (dash->underscore func) "(") "))")
        (flag-true "chess") )

(defun test-chess-table ()
        [ [ "next"                      2 2 ]
        ] )

